def div(n1, n2):
    try:
        n1, n2 = int(n1), int(n2)
        n3 = n1 / n2
    except ValueError:
        print("Ошибка значения")
    except ZeroDivisionError:
        print("Делить на нуль нельзя(")
    return round(n3, 2)


print(div(input("Введите первое число: "), input("Введите второе число: ")))